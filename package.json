{
  "name": "remixionnaire",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "db:mysql:push": "prisma db push --schema=./prisma/schema.mysql.prisma",
    "db:mysql:generate": "prisma generate --schema=./prisma/schema.mysql.prisma",
    "db:pscale:push": "prisma db push --schema=./prisma/schema.pscale.prisma",
    "db:pscale:generate": "prisma generate --schema=./prisma/schema.pscale.prisma",
    "db:push": "prisma db push --schema=./prisma/schema.sqlite.prisma",
    "db:generate": "prisma generate --schema=./prisma/schema.sqlite.prisma",
    "db:seed": "prisma db seed",
    "lambda:watch": "remix watch",
    "lambda:arc": "cross-env NODE_ENV=development arc sandbox",
    "dev:lambda": "run-s build:lambda && run-p lambda:*",
    "dev": "remix dev",
    "build": "remix build",
    "build:lambda": "cross-env REMIX_RUNTIME=arc remix build",
    "start": "cross-env NODE_ENV=production arc sandbox"
  },
  "dependencies": {
    "@nextui-org/react": "^1.0.0-beta.7",
    "@prisma/client": "^3.14.0",
    "@remix-run/architect": "^1.5.1",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/vercel": "^1.5.1",
    "@vercel/node": "^1.15.3",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "cuid": "^2.1.8",
    "dayjs": "^1.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.2.0",
    "tiny-invariant": "^1.2.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@architect/architect": "^10.3.3",
    "@faker-js/faker": "^6.3.1",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.14.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
